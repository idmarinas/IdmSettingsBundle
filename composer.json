{
	"name": "idmarinas/settings-bundle",
	"type": "symfony-bundle",
	"license": "BSD-3-Clause",
	"keywords": ["symfony-bundle", "settings-bundle", "user-settings"],
	"homepage": "https://github.com/idmarinas/settings-bundle",
	"authors": [
		{
			"name": "Iv√°n Diaz Marinas (IDMarinas)",
			"email": "35842929+idmarinas@users.noreply.github.com",
			"role": "Creator, Developer"
		}
	],
	"support": {
		"issues": "https://github.com/idmarinas/settings-bundle/issues"
	},
	"minimum-stability": "stable",
	"prefer-stable": true,
	"require": {
		"php": "^8.2",
		"symfony/cache": "^6.4 | ^7.0",
		"symfony/dependency-injection": "^6.4 | ^7.0",
		"symfony/framework-bundle": "^6.4 | ^7.0"
	},
	"require-dev": {
		"dama/doctrine-test-bundle": "^8.2",
		"doctrine/doctrine-fixtures-bundle": "^3.0 | ^4.0",
		"phpunit/phpunit": "^9.6",
		"rector/rector": "^1.2",
		"symfony/console": "^6.4 | ^7.0",
		"symfony/phpunit-bridge": "^7.2",
		"symfony/runtime": "^6.4 | ^7.0",
		"twig/twig": "^3.16",
		"zenstruck/foundry": "^2.3"
	},
	"config": {
		"allow-plugins": {
			"composer/package-versions-deprecated": true,
			"symfony/flex": true,
			"symfony/runtime": true
		},
		"optimize-autoloader": false,
		"preferred-install": {
			"*": "dist"
		},
		"sort-packages": true
	},
	"autoload": {
		"psr-4": {
			"Idm\\Bundle\\Settings\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"App\\": "app/src/",
			"Idm\\Bundle\\Settings\\Tests\\": "tests/",
			"DataFixtures\\": "fixtures/",
			"Factory\\": "factories/"
		}
	},
	"funding": [
		{
			"type": "paypal",
			"url": "https://www.paypal.me/idmarinas"
		},
		{
			"type": "github-sponsors",
			"url": "https://github.com/sponsors/idmarinas"
		}
	]
}
